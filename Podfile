# platform :ios, '9.0'

target 'Snapchat' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
	pod 'Firebase'
	pod 'Firebase/Core'
	pod 'Firebase/Auth'
	pod 'Firebase/Database'
	pod 'Firebase/Storage'
	pod 'SDWebImage'
  # Pods for Snapchat

  target 'SnapchatTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'SnapchatUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
